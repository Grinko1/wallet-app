<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet
            id="2024-11-06-1-create-table-wallet" author="developer">
        <createTable tableName="product">
            <column name="id" type="bigserial" >
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="name" type="varchar">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet
            id="2024-11-06-2-insert-wallets" author="developer">
        <insert tableName="product">
            <column  name="name"  value="name1"/>
        </insert>
        <insert tableName="product">
            <column  name="name"  value="name2"/>
        </insert>
        <insert tableName="product">
            <column  name="name"  value="name3"/>
        </insert>
        <insert tableName="product">
            <column  name="name"  value="name4"/>
        </insert>
    </changeSet>

</databaseChangeLog>